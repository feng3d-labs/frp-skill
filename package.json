{
  "name": "@feng3d/frp-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "frp 内网穿透 npm 包集合",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "node scripts/download-binaries.js",
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "prepublishOnly": "npm run build",
    "test": "vitest --run --exclude test/e2e.test.ts",
    "test:watch": "vitest --exclude test/e2e.test.ts",
    "test:coverage": "vitest --coverage --exclude test/e2e.test.ts",
    "test:e2e": "vitest --run test/e2e.test.ts",
    "test:unit": "npm run test --workspaces",
    "test:all": "vitest --run",
    "lint": "eslint packages/*/src",
    "format": "prettier --write \"packages/*/src/**/*.ts\""
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/tar": "^6.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wardenfeng/frp-skill.git"
  },
  "author": "feng3d",
  "license": "MIT"
}
