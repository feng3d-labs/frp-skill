{
  "name": "@feng3d/frpc",
  "version": "0.0.1",
  "description": "frp 客户端部署工具 - 支持 Linux/Windows/macOS",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "frpc": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format esm --clean --external unzipper --external @aws-sdk/client-s3 --external tar",
    "dev": "tsx src/cli.ts",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "download:binaries": "node scripts/download-binaries.js"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "execa": "^9.0.0",
    "unzipper": "^0.12.0",
    "tar": "^6.2.0",
    "inquirer": "^9.2.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "tsx": "^4.0.0"
  },
  "optionalDependencies": {
    "@feng3d/frpc-linux-x64": "1.0.0",
    "@feng3d/frpc-linux-arm64": "1.0.0",
    "@feng3d/frpc-win32-x64": "1.0.0",
    "@feng3d/frpc-darwin-x64": "1.0.0",
    "@feng3d/frpc-darwin-arm64": "1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "frp",
    "frpc",
    "内网穿透",
    "反向代理",
    "nat",
    "tunnel"
  ],
  "author": "feng3d",
  "license": "MIT"
}
